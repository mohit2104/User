# define prn(i) printf("%lld\n",(i))
# define cpr(i) cout<<(i)<<" "
# define cprn(i) cout<<(i)<<endl
# define csf(i) cin>>(i)
# define srt(v) sort( v.begin(), v.end() )
# define srtc(v, x) sort( v.begin(), v.end(), x)
using namespace std;

bool compare( void *a, void *b ){
	ll x = (ll *)a;
	ll y = (ll *)b;
	return x < y;
}

int main(){
	ll t;
	sf(t);
	REP(kr, t){
		printf("Case #%lld: ",kr);
	}
	return 0;
}
